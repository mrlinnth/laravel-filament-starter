{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "types": ["vite/client"],
    "jsx": "react-jsx",
    "baseUrl": "./resources/js",
    "typeRoots": ["./resources/js/@types", "node_modules/@types"],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@": [
        "./"
      ]
    }
  },
  "include": [
    "./resources/js"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
