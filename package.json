        "@types/ziggy-js": "^1.3.2",
        "@typescript-eslint/eslint-plugin": "^5.40.1",
        "@typescript-eslint/parser": "^5.40.1",
        "auto-changelog": "^2.4.0",
        "autoprefixer": "^10.4.12",
        "axios": "^0.27",
        "concurrently": "^7.4.0",
        "daisyui": "^2.31.0",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
